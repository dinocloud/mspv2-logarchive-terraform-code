{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::*:role/${name_role_lambda}"
            },
            "Action": [
                "s3:PutObject",
                "s3:*"
            ],
            "Resource": "arn:aws:s3:::${bucket_archive_name}/*"
        }
    ]
}